{
  "name": "vesta-onboarding",
  "version": "5.0.0",
  "description": "Vesta onboarding feature mock api",
  "author": "Jawad Farooqi <jawadfarooqi17@gmail.com>",
  "main": "index.js",
  "private": false,
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.0.0",
    "yarn": ">=1.5.1"
  },
  "scripts": {
    "start": "node index.js",
    "start:debug": "cross-env DEBUG=vesta-onboarding:* npm start",
    "lint": "esw *.js server config --color",
    "lint:watch": "npm lint -- --watch",
    "precommit": "npm lint && npm test",
    "test": "cross-env NODE_ENV=test ./node_modules/.bin/mocha --ui bdd --reporter spec --colors server --recursive",
    "test:watch": "npm test -- --watch",
    "test:coverage": "cross-env NODE_ENV=test ./node_modules/.bin/istanbul cover _mocha -- --ui bdd --reporter spec --colors server --recursive",
    "test:check-coverage": "npm test:coverage && istanbul check-coverage",
    "report-coverage": "coveralls < ./coverage/lcov.info"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/enablersinc/vesta2OnboardingService.git"
  },
  "keywords": [
    "express",
    "node",
    "node.js",
    "mongodb",
    "mongoose",
    "es6",
    "mocha",
    "istanbul",
    "REST",
    "API"
  ],
  "dependencies": {
    "bluebird": "3.5.5",
    "body-parser": "1.19.0",
    "compression": "1.7.4",
    "cookie-parser": "1.4.4",
    "cors": "2.8.5",
    "debug": "^2.6.9",
    "dotenv": "4.0.0",
    "express": "4.17.1",
    "express-jwt": "5.3.1",
    "express-validation": "1.0.2",
    "express-winston": "^2.5.0",
    "helmet": "^3.18.0",
    "http-status": "^1.3.2",
    "joi": "^10.6.0",
    "jsonwebtoken": "^7.4.3",
    "method-override": "^2.3.10",
    "mongoose": "^5.6.0",
    "morgan": "^1.9.1",
    "winston": "^2.4.4"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "commitizen": "3.1.1",
    "coveralls": "^3.0.0",
    "cross-env": "5.2.0",
    "cz-conventional-changelog": "2.1.0",
    "eslint": "^4.18.2",
    "eslint-config-airbnb-base": "^7.2.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-watch": "^2.1.14",
    "husky": "^0.14.3",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^3.5.3",
    "supertest": "^3.4.2",
    "supertest-as-promised": "4.0.2",
    "validate-commit-msg": "^2.14.0"
  },
  "license": "MIT",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
